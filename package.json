{
  "name": "hodlwatch",
  "homepage": "https://hodl.watch",
  "version": "0.3.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/belaczek/hodlwatch.git"
  },
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "serve dist",
    "dev": "next src",
    "build": "next build src && npm run export",
    "export": "next export src -o './dist'",
    "deploy": "gh-pages -d dist",
    "test": "cross-env test=1 jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "format": "prettier-eslint --write 'src/**/*.js'",
    "format:single": "prettier-eslint --write",
    "predeploy": "npm run export",
    "precommit": "lint-staged",
    "analyze": "cross-env BUNDLE_ANALYZE=browser next build src"
  },
  "lint-staged": {
    "*.js": [
      "npm run format:single --",
      "eslint --max-warnings=0",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "0.19.2",
    "big.js": "5.2.2",
    "bignumber.js": "^8.1.1",
    "bloomer": "0.6.5",
    "bulma": "0.8.0",
    "bulma-switch": "2.0.0",
    "ccxt": "^1.22.28",
    "classnames": "2.2.6",
    "color-hash": "1.0.3",
    "crypto-js": "3.1.9-1",
    "date-fns": "1.30.1",
    "ethereumjs-abi": "0.6.8",
    "ethereumjs-util": "6.2.0",
    "lodash": "4.17.15",
    "next": "9.2.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "7.1.3",
    "react-toastify": "5.5.0",
    "recharts": "1.8.5",
    "recompose": "^0.30.0",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "serve": "11.3.0",
    "web3": "1.2.6"
  },
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "@zeit/next-bundle-analyzer": "0.1.2",
    "@zeit/next-sass": "1.0.1",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.15.0",
    "eslint-config-react-app": "^3.0.7",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "gh-pages": "^2.0.1",
    "husky": "^0.14.3",
    "jest": "^24.1.0",
    "lint-staged": "^7.3.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "prettier-eslint": "9.0.1",
    "prettier-eslint-cli": "5.0.0"
  }
}
